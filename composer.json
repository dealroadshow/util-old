{
  "name": "dealroadshow/granule-util",
  "description": "Granule Utilities fork",
  "keywords": [
    "enum",
    "tree",
    "array",
    "data",
    "collection",
    "map"
  ],
  "type": "library",
  "require": {
    "php": ">=8.2"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:dealroadshow/phpcs-finsight-ruleset"
    }
  ],
  "require-dev": {
    "dealroadshow/phpcs-finsight-ruleset": "^2.2",
    "phpunit/phpunit": "^9",
    "vimeo/psalm": "^5.26.1",
    "orklah/psalm-insane-comparison": "^2.0",
    "marartner/psalm-no-empty": "^2.0",
    "orklah/psalm-strict-equality": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.64"
  },
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene Bogachov",
      "email": "eugene.bogachov@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Granule\\Util\\": "lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Granule\\Tests\\Util\\": "tests"
    }
  }
}
